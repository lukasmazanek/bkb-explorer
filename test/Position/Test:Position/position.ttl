@prefix : <http://example.org/ontology/Test:Position/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

: a owl:Ontology ;
    rdfs:label "Position Ontology" ;
    rdfs:comment "Domain ontology for Test:Position" .

:Bond_position a owl:Class ;
    rdfs:label "Bond position" ;
    rdfs:comment "position in some fund, which may be defined in terms of fund units" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/FundPosition> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/FundPosition> .

:Cash_position a owl:Class ;
    rdfs:label "Cash position" ;
    rdfs:comment "investor's stake, i.e., a holding, in a particular asset (such as an individual security)" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> .

:Closed_position a owl:Class ;
    rdfs:label "Closed position" ;
    rdfs:comment "investor's stake, i.e., a holding, in a particular asset (such as an individual security)" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> .

:Custody_fee a owl:Class ;
    rdfs:label "Custody fee" ;
    rdfs:comment "A contract between an investor and a custodian bank for the safekeeping of securities and other assets." ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesHoldings/CustodyAgreement> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesHoldings/CustodyAgreement> .

:Long_position a owl:Class ;
    rdfs:label "Long position" ;
    rdfs:comment "investor's stake, i.e., a holding, in a particular asset (such as an individual security)" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> .

:Open_position a owl:Class ;
    rdfs:label "Open position" ;
    rdfs:comment "investor's stake, i.e., a holding, in a particular asset (such as an individual security)" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> .

:Securities_position a owl:Class ;
    rdfs:label "Securities position" ;
    rdfs:comment "A holding of a security or commodity, either long or short, by an investor or trader." ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesHoldings/Position> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesHoldings/Position> .

:Short_position a owl:Class ;
    rdfs:label "Short position" ;
    rdfs:comment "investor's stake, i.e., a holding, in a particular asset (such as an individual security)" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> ;
    skos:closeMatch <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> .

<https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/Account> a owl:Class ;
    rdfs:label "Account" ;
    rdfs:comment "container for records associated with a business arrangement for regular transactions and services" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/Account> ;
    owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/Account> .

<https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Order> a owl:Class ;
    rdfs:label "Order" ;
    rdfs:comment "An instruction from a customer or client to buy or sell securities, commodities, or other financial instruments." ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Order> ;
    owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Order> .

<https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Assessments/MarketValue> a owl:Class ;
    rdfs:label "Market value" ;
    rdfs:comment "price an asset would sell for in the market" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Assessments/MarketValue> ;
    owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Assessments/MarketValue> .

<https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/EquityPosition> a owl:Class ;
    rdfs:label "Equity position" ;
    rdfs:comment "position in an equity instrument" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/EquityPosition> ;
    owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/EquityPosition> .

<https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/FundPosition> a owl:Class ;
    rdfs:label "Fund position" ;
    rdfs:comment "position in some fund, which may be defined in terms of fund units" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/FundPosition> ;
    owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/FundPosition> .

<https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> a owl:Class ;
    rdfs:label "Position" ;
    rdfs:comment "investor's stake, i.e., a holding, in a particular asset (such as an individual security)" ;
    rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> ;
    owl:equivalentClass <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/Position> .

